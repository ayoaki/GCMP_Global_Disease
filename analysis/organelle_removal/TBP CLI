#2 samples in feature table not in metadata, have to be filtered before we can go further

qiime feature-table filter-samples --i-table gcmp_feature_table.qza --m-metadata-file GCMP_EMP_map_r28.txt --o-filtered-table GCMP_ft.qza

qiime feature-table filter-samples --i-table GCMP_mito_chloro_filtered.qza --m-metadata-file GCMP_EMP_map_r28.txt --o-filtered-table GCMP_GG_ft.qza 

qiime taxa barplot --i-table GCMP_ft.qza --i-taxonomy gcmp_taxonomy.qza --m-metadata-file GCMP_EMP_map_r28.txt --o-visualization zaneveld_tbp.qzv

qiime taxa barplot --i-table GCMP_GG_ft.qza --i-taxonomy GG99+_taxonomy.qza --m-metadata-file GCMP_EMP_map_r28.txt --o-visualization gg+_tbp.qzv

#the full TBPs crash firefox tabs and chrome struggles even at taxonomy level 1, let alone 5.

qiime feature-table filter-samples --i-table GCMP_ft.qza --m-metadata-file GCMP_EMP_map_r28.txt --p-where "tissue_compartment='M'" --o-filtered-table GCMP_M_ft.qza

qiime feature-table filter-samples --i-table GCMP_ft.qza --m-metadata-file GCMP_EMP_map_r28.txt --p-where "tissue_compartment='T'" --o-filtered-table GCMP_T_ft.qza

qiime feature-table filter-samples --i-table GCMP_ft.qza --m-metadata-file GCMP_EMP_map_r28.txt --p-where "tissue_compartment='S'" --o-filtered-table GCMP_S_ft.qza

qiime feature-table filter-samples --i-table GCMP_GG_ft.qza --m-metadata-file GCMP_EMP_map_r28.txt --p-where "tissue_compartment='M'" --o-filtered-table GCMP_GG_M_ft.qza

qiime feature-table filter-samples --i-table GCMP_GG_ft.qza --m-metadata-file GCMP_EMP_map_r28.txt --p-where "tissue_compartment='T'" --o-filtered-table GCMP_GG_T_ft.qza

qiime feature-table filter-samples --i-table GCMP_GG_ft.qza --m-metadata-file GCMP_EMP_map_r28.txt --p-where "tissue_compartment='S'" --o-filtered-table GCMP_GG_S_ft.qza

qiime taxa barplot --i-table GCMP_M_ft.qza --i-taxonomy gcmp_taxonomy.qza --m-metadata-file GCMP_EMP_map_r28.txt --o-visualization zaneveld_M_tbp.qzv

qiime taxa barplot --i-table GCMP_T_ft.qza --i-taxonomy gcmp_taxonomy.qza --m-metadata-file GCMP_EMP_map_r28.txt --o-visualization zaneveld_T_tbp.qzv

qiime taxa barplot --i-table GCMP_S_ft.qza --i-taxonomy gcmp_taxonomy.qza --m-metadata-file GCMP_EMP_map_r28.txt --o-visualization zaneveld_S_tbp.qzv

qiime taxa barplot --i-table GCMP_GG_M_ft.qza --i-taxonomy GG99+_taxonomy.qza --m-metadata-file GCMP_EMP_map_r28.txt --o-visualization gg+_M_tbp.qzv

qiime taxa barplot --i-table GCMP_GG_T_ft.qza --i-taxonomy GG99+_taxonomy.qza --m-metadata-file GCMP_EMP_map_r28.txt --o-visualization gg+_T_tbp.qzv

qiime taxa barplot --i-table GCMP_GG_S_ft.qza --i-taxonomy GG99+_taxonomy.qza --m-metadata-file GCMP_EMP_map_r28.txt --o-visualization gg+_S_tbp.qzv

